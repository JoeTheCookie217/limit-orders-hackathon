{
    "name": "@dusalabs/periphery",
    "version": "0.0.1",
    "description": "",
    "main": "index.js",
    "scripts": {
        "postinstall": "npm link @dusalabs/core",
        "test": "asp --summary",
        "build": "npx massa-as-compile",
        "build:main": "asc assembly/contracts/main.ts -o build/main.wasm --transform @massalabs/as-transformer",
        "build:migrate": "asc assembly/contracts/migrate.ts -o build/migrate.wasm --transform @massalabs/as-transformer",
        "build:multicall": "asc assembly/contracts/Multicall.ts -o build/Multicall.wasm --transform @massalabs/as-transformer",
        "deploy": "tsx src/deploy.ts",
        "prettier": "prettier assembly//**/*.ts --check && as-prettier --check assembly",
        "prettier:fix": "prettier assembly//**/*.ts --write && as-prettier --write assembly",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "fmt:check": "npm run prettier && npm run lint",
        "fmt": "npm run prettier:fix && npm run lint:fix"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@as-pect/cli": "^8.1.0",
        "@assemblyscript/loader": "^0.27.29",
        "@massalabs/as-types": "^2.0.0",
        "@massalabs/as-transformer": "^0.4.0",
        "@massalabs/eslint-config": "^0.0.11",
        "@massalabs/massa-as-sdk": "^3.0.1-dev.20250115142116",
        "@massalabs/massa-sc-compiler": "^0.2.1-dev",
        "@massalabs/massa-sc-deployer": "^1.3.0",
        "@massalabs/massa-web3": "^5.0.0",
        "@massalabs/prettier-config-as": "^0.0.2",
        "@massalabs/sc-standards": "^1.3.0",
        "@types/node": "^18.11.10",
        "as-bignum": "github:massalabs/as-bignum#0105eb596b2fa707c00712e811a2efdfcb8a9848",
        "assemblyscript": "^0.27.29",
        "assemblyscript-prettier": "^1.0.7",
        "dotenv": "^16.0.3",
        "prettier": "^2.8.1",
        "tslib": "^2.4.0",
        "tsx": "^4.7.0",
        "typescript": "^4.8.4"
    },
    "type": "module",
    "prettier": "@massalabs/prettier-config-as",
    "engines": {
        "node": ">=16"
    },
    "dependencies": {
        "tsx": "^4.16.2"
    }
}
